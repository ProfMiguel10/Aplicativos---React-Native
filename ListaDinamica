import React from 'react';
import { View, Text, FlatList } from 'react-native';

export default function App() {
  const dados = [
    { id: '1', nome: 'Miguel' },
    { id: '2', nome: 'Maria' }
  ];

  return (
    <View>
      <FlatList
        data={dados}
        keyExtractor={item => item.id}
        renderItem={({ item }) => (
          <Text>{item.nome}</Text>
        )}
      />
    </View>
  );
}
